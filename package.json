{
	"name": "hmda-hero",
	"description": "HMDA Hero",
	"version": "0.0.1",
	"scripts": {
		"dev": "sapper dev",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"validate": "office-toolbox validate -m Manifest/Prod/manifest-prod.xml",
		"validate:local": "office-toolbox validate -m Manifests/localhost/manifest-localhost.xml",
		"certs": "office-addin-dev-certs install",
		"certs:verify": "office-addin-dev-certs verify",
		"certs:uninstall": "office-addin-dev-certs uninstall"
	},
	"dependencies": {
		"compression": "^1.7.1",
		"office-toolbox": "^0.3.0",
		"polka": "next",
		"sirv": "^0.4.6"
	},
	"devDependencies": {
		"@babel/core": "^7.11.1",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/runtime": "^7.11.2",
		"@rollup/plugin-replace": "^2.3.1",
		"@types/office-js": "^1.0.178",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"npm-run-all": "^4.1.5",
		"office-addin-debugging": "^3.0.33",
		"office-addin-dev-certs": "^1.5.4",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"rollup": "^1.32.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-plugin-svelte": "^6.1.1",
		"rollup-plugin-terser": "^4.0.4",
		"sapper": "^0.27.16",
		"svelte": "^3.29.6"
	}
}
